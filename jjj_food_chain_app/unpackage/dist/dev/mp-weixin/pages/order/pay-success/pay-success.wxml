<block wx:if="{{!loadding}}"><view class="pay-success"><view class="success-icon d-c-c d-c"><text class="iconfont icon-queren"></text><text class="name">支付成功</text></view><view class="success-price d-c-c">￥<text class="num">{{detail.pay_price}}</text></view><block wx:if="{{detail.points_bonus>0}}"><view class="order-info mt30 f28"><view class="d-b-c p20 border-b"><text class="gray9">积分赠送</text><text class="gray3">{{detail.points_bonus}}</text></view></view></block><view class="success-btns d-b-c"><button class="flex-1 mr10" type="default" data-event-opts="{{[['tap',[['goHome']]]]}}" bindtap="__e">返回首页</button><button class="flex-1 ml10" type="primary" data-event-opts="{{[['tap',[['goMyorder',['$event']]]]]}}" bindtap="__e">我的订单</button></view></view></block>